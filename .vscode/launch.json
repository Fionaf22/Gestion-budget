{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch & Profile built-in server",
            "type": "php",
            "request": "launch",
            "noDebug": true,
            "profile": true,
            "runtimeArgs": [
                "-S",
                "localhost:8000",
                "-t",
                "."
            ],
            "port": 9003,
            "serverReadyAction": {
                "action": "openExternally"
            }
        },
        {
            "name": "Launch built-in server and Debug",
            "detail": "Launches PHP's built-in development server and starts debugging. (Root: \".\")",
            "type": "php",
            "request": "launch",
            "noDebug": false,
            "runtimeArgs": [
                "-S",
                "localhost:80",
                "-t",
                "."
            ],
            "serverReadyAction": {
                "action": "openExternally"
            }
        }
    ]
}